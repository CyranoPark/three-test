{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "ignorePatterns": ["webpack.config.js"],
  "rules": {
    "prettier/prettier": "error"
  }
}
